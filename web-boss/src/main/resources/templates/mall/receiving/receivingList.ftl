                        <!-- 如果当前为第一页时,就没有上一页这个超链接显示 -->
                        <#if receivingPageBean ?? && receivingPageBean.pageNum == 1 && receivingPageBean.pageNum != receivingPageBean.totalPage>
                            <#list receivingPageBean.start..receivingPageBean.end as i>
                                <#if receivingPageBean.pageNum == i>
                                    ${i}
                                <#else >
                                    <a href="/receiving/findReceiving?page=${i}">${i}</a>
                                </#if>
                            </#list>
                            <a href="/receiving/findReceiving?page=${receivingPageBean.pageNum + 1}">下一页</a>
                        </#if>
                        <!-- 如果当前不是第一页,也不是最后一页时,则有上一页跟下一页 -->
                        <#if receivingPageBean.pageNum &gt;1 && receivingPageBean.pageNum &lt; receivingPageBean.totalPage>
                            <a href="/receiving/findReceiving?page=${receivingPageBean.pageNum-1}">上一页</a>
                            <#list receivingPageBean.start..receivingPageBean.end as i>
                                <#if receivingPageBean.pageNum == i>
                                    ${i}
                                <#else >
                                    <a href="/receiving/findReceiving?page=${i}">${i}</a>
                                </#if>
                            </#list>
                            <a href="/receiving/findReceiving?page=${receivingPageBean.pageNum + 1}">下一页</a>
                        </#if>
                        <!-- 如果当前为最后一页时,则没有下一页 -->
                        <#if receivingPageBean.pageNum == receivingPageBean.totalPage && receivingPageBean.pageNum != 1>
                            <a href="/receiving/findReceiving?page=${receivingPageBean.pageNum-1}">上一页</a>
                            <#list receivingPageBean.start..receivingPageBean.end as i>
                                <#if receivingPageBean.pageNum == i>
                                    ${i}
                                <#else >
                                    <a href="/receiving/findReceiving?page=${i}">${i}</a>
                                </#if>
                            </#list>
                        </#if>
                        <!-- 如果当前为第一页,且为最后一页时没有上一页跟下一页 -->
                        <#if receivingPageBean.pageNum == receivingPageBean.totalPage && receivingPageBean.pageNum == 1>
                            <#list receivingPageBean.start..receivingPageBean.end as i>
                                <#if receivingPageBean.pageNum == i>
                                    ${i}
                                <#else >
                                    <a href="/receiving/findReceiving?page=${i}">${i}</a>
                                </#if>
                            </#list>
                        </#if>
                        <a href="/receiving/findReceiving?page=${receivingPageBean.totalPage}">尾页</a>
                    </#if>
                    共${receivingPageBean.totalRecord}条
                </td>
            </tr></table>
    </div>
    <!-- END LIST -->
</div>
<script>
    function reset_search() {
        var url = $('#search_form form ').attr('action');
        window.location.href = url;
    }
</script>
<script type="text/javascript">
    document.onkeydown=function (e) {
        e=window.event||e;
        var key = e.keyCode;
        if ((e.ctrlKey && key == 82) || key == 116) {
            parent.MainIframe.location.reload();
            if(document.all) {
                e.keyCode = 0;
                e.returnValue = false;
            }else {
                e.cancelBubble = true;
                e.preventDefault();
            }
        }
    }
    /**
     * 初始化对象
     */
//表格样式
    $(document).ready(function(){
        admin.bindTrOn();});
</script>

</body>
</html>